<?xml version="1.0" encoding="UTF-8"?>
<AUTOSAR xmlns="http://autosar.org/schema/r4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://autosar.org/schema/r4.0 autosar_00050.xsd">
    <AR-PACKAGES>
        <AR-PACKAGE>
            <SHORT-NAME>ExecutionManifest</SHORT-NAME>
            <ELEMENTS>
                <COMMUNICATION-CLUSTER>
                    <ETHERNET-PHYSICAL-CHANNEL>
                        <SHORT-NAME>Localhost</SHORT-NAME>
                        <NETWORK-ENDPOINTS>
                            <NETWORK-ENDPOINT>
                                <SHORT-NAME>RpcServerEP</SHORT-NAME>
                                <NETWORK-ENDPOINT-ADDRESSES>
                                    <IPV-4-CONFIGURATION>
                                        <IPV-4-ADDRESS>127.0.0.1</IPV-4-ADDRESS>
                                    </IPV-4-CONFIGURATION>
                                </NETWORK-ENDPOINT-ADDRESSES>
                            </NETWORK-ENDPOINT>
                        </NETWORK-ENDPOINTS>
                    </ETHERNET-PHYSICAL-CHANNEL>
                    <PROTOCOL-VERSION>1</PROTOCOL-VERSION>
                </COMMUNICATION-CLUSTER>
                <ETHERNET-COMMUNICATION-CONNECTOR>
                    <SHORT-NAME>RpcServerConn</SHORT-NAME>
                    <AP-APPLICATION-ENDPOINTS>
                        <AP-APPLICATION-ENDPOINT>
                            <SHORT-NAME>ServerUnicastTcp</SHORT-NAME>
                            <TP-CONFIGURATION>
                                <TCP-TP>
                                    <TCP-TP-PORT>
                                        <PORT-NUMBER>8080</PORT-NUMBER>
                                    </TCP-TP-PORT>
                                </TCP-TP>
                            </TP-CONFIGURATION>
                        </AP-APPLICATION-ENDPOINT>
                    </AP-APPLICATION-ENDPOINTS>
                </ETHERNET-COMMUNICATION-CONNECTOR>
                <FUNCTION-GROUPS>
                    <FUNCTION-GROUP>
                        <SHORT-NAME>MachineFG</SHORT-NAME>
                        <MODE-DECLARATION-GROUP>
                            <INITIAL-MODE-REF DEST="MODE-DECLARATION">Off</INITIAL-MODE-REF>
                            <MODE-DECLARATIONS>
                                <MODE-DECLARATION>
                                    <SHORT-NAME>Off</SHORT-NAME>
                                </MODE-DECLARATION>
                                <MODE-DECLARATION>
                                    <SHORT-NAME>StartUp</SHORT-NAME>
                                </MODE-DECLARATION>
                            </MODE-DECLARATIONS>
                        </MODE-DECLARATION-GROUP>
                    </FUNCTION-GROUP>
                </FUNCTION-GROUPS>        
            </ELEMENTS>
        </AR-PACKAGE>
    </AR-PACKAGES>
</AUTOSAR>